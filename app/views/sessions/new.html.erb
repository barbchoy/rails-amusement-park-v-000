<h1>Sign In</h1>
<%= form_for(:session, url: signin_path) do |f| %>

  <%= f.label :name %>
  <%= f.text_field "user_name" %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Log in" %>
<% end %>
<br>
<p>New user? <%= link_to "Sign up now!", signup_path %></p>
